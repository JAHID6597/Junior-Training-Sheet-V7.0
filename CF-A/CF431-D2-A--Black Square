#include<bits/stdc++.h>
using namespace std;

//#define debug

int main()
{
#ifdef debug
    freopen("input.txt","r",stdin);
#endif

    string s;
    int a[5],sum=0,p;
    for(int i=1;i<=4;i++)
    {
        cin>>a[i];
    }cin>>s;
    for(int i=0;i<s.length();i++)
    {
        p = s[i] - '0';
        sum += a[p];
    }
    cout<<sum;
    return 0;
}
