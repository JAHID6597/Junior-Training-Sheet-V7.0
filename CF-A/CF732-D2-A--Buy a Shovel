#include <bits/stdc++.h>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);

    // freopen("input.txt", "r", stdin);
    // freopen("output.txt", "w", stdout);

    int k,r,x,a=1;
    cin>>k>>r;
    for(int i=1;i<10;i++){
        if(k*i % 10 == 0 || k*i % 10 == r){
            a = i;
            break;
        }
    }
    cout<<a<<endl;
    return 0;
}
