    #include<bits/stdc++.h>
    using namespace std;
     
    //#define debug
     
    int main()
    {
     
    #ifdef debug
        freopen("C:/Users/jahid/Desktop/New Text Document.txt","r",stdin);
    #endif
     
        int t,A=0,D=0;
        cin>>t;
        string s;
        cin>>s;
        for(int i=0;i<t;i++)
        {
            if(s[i]=='A')A++;
            else if(s[i]=='D')D++;
        }
        if(A>D)cout<<"Anton"<<endl;
        else if(A<D)cout<<"Danik"<<endl;
        else if(A==D)cout<<"Friendship"<<endl;
        return 0;
    }
