    #include<bits/stdc++.h>
    using namespace std;
     
    //#define debug
     
    int main()
    {
     
    #ifdef debug
        freopen("C:/Users/jahid/Desktop/New Text Document.txt","r",stdin);
    #endif
        int n,k,p,c=0;
        cin>>n>>k;
        char a[26]= {'a','b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t',
                     'u','v','w','x','y','z'
                    };
        p=n/k;
        while(p--)
        {
            for(int i=0; i<k; i++)
            {
                cout<<a[i];c++;
            }
        }
        for(int i=0; i<n-c; i++)
        {
            cout<<a[i];
        }
    }
